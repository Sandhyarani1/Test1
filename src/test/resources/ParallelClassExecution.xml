<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
	<suite name="Core-Regression" verbose="5" parallel="methods" thread-count="2">
		<parameter name="runLocation" value="grid" />
		<parameter name="environment" value="qa" />
		<parameter name="operatingSystem" value="WINDOWS" />
		<parameter name="logLevel" value="3"/>

		<test name="Browse Locations In Chrome">
			<parameter name="browserUnderTest" value="Firefox" />
      		<classes>
      			<class name="com.accuweather.glacier.www.browseLocations.Test_browseLocationsPage"/>
      		</classes>
		</test>
		<test name="Daily Lists In Chrome">
			<parameter name="browserUnderTest" value="Firefox" />
      		<classes>
      			<class name="com.accuweather.glacier.www.dailyList.Test_DailyListPage"/>
      		</classes>
		</test>
		
	</suite>
